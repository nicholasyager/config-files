Config { font = "xft:inconsolata:size=10:antialias=true"
, fgColor = "#ffb001"
, bgColor = "black"
, position = Top
, lowerOnStart = True
, commands = [ Run Weather "KROC" ["-t","<tempF>F","-L","45","-H","80","--normal","#33ff33","--high","red","--low","lightblue"] 36000
             , Run Wireless "wlp2s0" ["-t", "<essid>:<quality>"] 10
             , Run Cpu ["-L","3","-H","50","--normal","lightblue","--high","red"] 10
             , Run Com "~/.bin/volume.sh" []"vol" 10
             , Run Memory ["-t","Mem: <usedratio>%"] 10
             , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
    	     , Run StdinReader 
             ]
, sepChar = "%"
, alignSep = "}{"
, template = "%cpu% | %memory% | %vol% | <fc=red>%wlp2s0wi% </fc> } %StdinReader% { %KROC% | %date%"
}
