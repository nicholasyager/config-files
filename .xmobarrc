Config { font = "xft:inconsolata:size=10:antialias=true"
, fgColor = "#ffb001"
, bgColor = "black"
, position = Top
, lowerOnStart = True
, commands = [ Run Wireless "wlp2s0" ["-t", "<essid>:<quality>"] 10
             , Run Com "/home/nicholas/.bin/power.sh" [] "battery" 600
             , Run Com "/home/nicholas/.bin/volume.sh" [] "vol" 10
             , Run Com "/home/nicholas/.bin/weather.py" [] "weath" 8000
             , Run Cpu ["-L","3","-H","50","--normal","lightblue","--high","red", "-t","C: <total>%"] 10
             , Run Memory ["-t","M: <usedratio>%"] 10
             , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
    	     , Run StdinReader 
             ]
, sepChar = "%"
, alignSep = "}{"
, template = " %cpu% | %memory% | %vol% | %battery% | <fc=red>%wlp2s0wi% </fc> } %StdinReader% { %weath% | %date% "
}
